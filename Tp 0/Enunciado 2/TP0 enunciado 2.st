Object subclass: #Alumno	instanceVariableNames: 'nombre padron notas'	classVariableNames: ''	package: 'TP0'!!Alumno methodsFor: 'as yet unclassified'!obtenerPadron	^padron.! !!Alumno methodsFor: 'as yet unclassified'!establecerNota: unNumero.	notas add: unNumero .! !!Alumno methodsFor: 'as yet unclassified'!establecerNombre: unNombre 	nombre := unNombre .! !!Alumno methodsFor: 'as yet unclassified'!calcularPromedio	^notas average.! !!Alumno methodsFor: 'as yet unclassified'!establecerPadron: unNumero	padron := unNumero .! !!Alumno methodsFor: 'initialization'!initialize 	notas := OrderedCollection new.! !Object subclass: #Curso	instanceVariableNames: 'alumnos nombre'	classVariableNames: ''	package: 'TP0'!!Curso methodsFor: 'initialization'!initialize	alumnos  := OrderedCollection new.! !!Curso methodsFor: 'as yet unclassified'!inscribirAlumno: unAlumno conNumeroDePadron: unPadron	unAlumno establecerPadron: unPadron . 		alumnos add: unAlumno.! !!Curso methodsFor: 'as yet unclassified'!promedioDe: unPadron|promedio|  alumnos do: [ :alumno|		((alumno obtenerPadron )= unPadron ) ifTrue: [promedio :=alumno calcularPromedio] 	].^promedio.! !!Curso methodsFor: 'as yet unclassified'!calificarA: unPadron  conNota: unaNota	alumnos do: [ :alumno|		((alumno obtenerPadron )= unPadron ) ifTrue: [alumno establecerNota: unaNota.] 	].		! !!Curso methodsFor: 'as yet unclassified'!establecerNombre: unNombre  	nombre := unNombre .! !!Curso methodsFor: 'as yet unclassified'!aprobo: unPadron 	^((self promedioDe: unPadron) > 4).! !