Object subclass: #ComisionCurricularPermanente	instanceVariableNames: 'expedientes indice'	classVariableNames: ''	package: 'TP01'!!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 18:40'!votarAFavor 	(expedientes at: indice) votarAFavor.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 19:03'!procesarExpediente	|aux|	aux := (((expedientes at:indice) votosAFavor) > ((expedientes at:indice) votosEnContra)).	aux ifTrue: [(expedientes at:indice) establecerEstado: 'Aprobado'] ifFalse:[(expedientes at:indice) establecerEstado: 'Rechazado'].	indice := indice + 1.	^(expedientes at:(indice-1)) 		! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 18:40'!votarEnContra	(expedientes at: indice) votarEnContra .! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 17:18'!recibirExpediente: unExpediente 	expedientes add: unExpediente.! !!ComisionCurricularPermanente methodsFor: 'initialization' stamp: 'Joaquin 4/18/2020 18:30'!initialize	expedientes := OrderedCollection new.	indice := 1.! !Object subclass: #Expediente	instanceVariableNames: 'nombre estado votosAFavor votosEnContra'	classVariableNames: ''	package: 'TP01'!!Expediente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 17:09'!estado	^estado.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 16:54'!establecerNombre: unNombre	nombre := unNombre .! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 18:45'!establecerEstado: unEstado	estado := unEstado.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 18:33'!votarEnContra	votosEnContra  := votosEnContra + 1.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Joaquin 4/18/2020 18:33'!votarAFavor	votosAFavor := votosAFavor + 1.! !!Expediente methodsFor: 'initialization' stamp: 'Joaquin 4/18/2020 18:32'!initialize	estado := 'Iniciado'.	votosAFavor := 0.	votosEnContra := 0.! !!Expediente methodsFor: 'accessing' stamp: 'Joaquin 4/18/2020 18:34'!votosEnContra	^votosEnContra .! !!Expediente methodsFor: 'accessing' stamp: 'Joaquin 4/18/2020 18:34'!votosAFavor	^votosAFavor.! !